      "item": [
        {
          "name": "Create Space",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Authorization", "value": "Bearer {{authToken}}" },
              { "key": "Content-Type", "value": "application/json" }
            ],
            "url": {
              "raw": "{{baseUrl}}/api/spaceportal/spaces",
              "host": ["{{baseUrl}}"],
              "path": ["api", "spaceportal", "spaces"]
            },
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"FlashSpace Delhi\",\n  \"city\": \"Delhi\",\n  \"location\": \"Connaught Place\",\n  \"totalSeats\": 100,\n  \"availableSeats\": 80,\n  \"meetingRooms\": 5,\n  \"cabins\": 10,\n  \"status\": \"ACTIVE\"\n}"
            }
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test('Status is 201', function () {",
                  "    pm.response.to.have.status(201);",
                  "});",
                  "",
                  "const res = pm.response.json();",
                  "pm.test('Envelope success true', function () {",
                  "    pm.expect(res.success).to.eql(true);",
                  "});",
                  "",
                  "if(res.data && res.data._id){",
                  "    pm.collectionVariables.set('spaceId', res.data._id);",
                  "}"
                ]
              }
            }
          ]
        },
        {
          "name": "Get Spaces (List)",
          "request": {
            "method": "GET",
            "header": [
              { "key": "Authorization", "value": "Bearer {{authToken}}" }
            ],
            "url": {
              "raw": "{{baseUrl}}/api/spaceportal/spaces?page=1&limit=10&includeDeleted=false",
              "host": ["{{baseUrl}}"],
              "path": ["api", "spaceportal", "spaces"],
              "query": [
                { "key": "page", "value": "1" },
                { "key": "limit", "value": "10" },
                { "key": "includeDeleted", "value": "false" }
